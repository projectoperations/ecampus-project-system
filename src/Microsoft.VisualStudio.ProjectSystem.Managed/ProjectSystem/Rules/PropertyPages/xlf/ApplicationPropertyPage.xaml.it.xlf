<?xml version="1.0" encoding="utf-8"?>
<xliff xmlns="urn:oasis:names:tc:xliff:document:1.2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.2" xsi:schemaLocation="urn:oasis:names:tc:xliff:document:1.2 xliff-core-1.2-transitional.xsd">
  <file datatype="xml" source-language="en" target-language="it" original="../ApplicationPropertyPage.xaml">
    <body>
      <trans-unit id="BoolProperty|TargetMultipleFrameworks|Description">
        <source>Build this project for multiple target frameworks.</source>
        <target state="translated">Compila questo progetto per più framework di destinazione.</target>
        <note />
      </trans-unit>
      <trans-unit id="BoolProperty|TargetMultipleFrameworks|DisplayName">
        <source>Target multiple frameworks</source>
        <target state="translated">Più framework di destinazione</target>
        <note />
      </trans-unit>
      <trans-unit id="Category|General|Description">
        <source>General settings for the application.</source>
        <target state="translated">Impostazioni generali per l'applicazione.</target>
        <note />
      </trans-unit>
      <trans-unit id="Category|General|DisplayName">
        <source>General</source>
        <target state="translated">Generale</target>
        <note />
      </trans-unit>
      <trans-unit id="Category|Resources|Description">
        <source>Resource settings for the application.</source>
        <target state="translated">Impostazioni delle risorse per l'applicazione.</target>
        <note />
      </trans-unit>
      <trans-unit id="Category|Resources|DisplayName">
        <source>Resources</source>
        <target state="translated">Risorse</target>
        <note />
      </trans-unit>
      <trans-unit id="DynamicEnumProperty|StartupObject|Description">
        <source>Defines the entry point to be called when the application loads. Generally this is set either to the main form in your application or to the 'Main' procedure that should run when the application starts. Class libraries do not define an entry point.</source>
        <target state="translated">Definisce il punto di ingresso da chiamare quando l'applicazione viene caricata. In genere viene impostato sul modulo principale dell'applicazione o sulla routine 'Main' che deve essere eseguita all'avvio dell'applicazione. Le librerie di classi non definiscono un punto di ingresso.</target>
        <note />
      </trans-unit>
      <trans-unit id="DynamicEnumProperty|StartupObject|DisplayName">
        <source>Startup object</source>
        <target state="translated">Oggetto di avvio</target>
        <note />
      </trans-unit>
      <trans-unit id="DynamicEnumProperty|TargetFramework|Description">
        <source>Specifies the version of .NET that the application targets. This option can have different values depending on which versions of .NET are installed on your computer.</source>
        <target state="translated">Specifica la versione di .NET di destinazione dell'applicazione. Questa opzione può avere valori diversi a seconda delle versioni di .NET installate nel computer.</target>
        <note />
      </trans-unit>
      <trans-unit id="DynamicEnumProperty|TargetFramework|DisplayName">
        <source>Target framework</source>
        <target state="translated">Framework di destinazione</target>
        <note />
      </trans-unit>
      <trans-unit id="DynamicEnumProperty|TargetFramework|Metadata|SearchTerms">
        <source>TFM</source>
        <target state="new">TFM</target>
        <note />
      </trans-unit>
      <trans-unit id="EnumProperty|ApplicationManifestKind|Description">
        <source>Selects a manifest generation option when the application runs on Windows Vista or later under User Account Control (UAC). Options include a default manifest, no manifest, or a custom manifest provided by your project.</source>
        <target state="translated">Consente di selezionare un'opzione di generazione del manifesto quando l'applicazione viene eseguita in Windows Vista o versioni successive in Controllo dell'account utente. Le opzioni includono un manifesto predefinito, nessun manifesto o un manifesto personalizzato fornito dal progetto.</target>
        <note />
      </trans-unit>
      <trans-unit id="EnumProperty|ApplicationManifestKind|DisplayName">
        <source>Manifest</source>
        <target state="translated">Manifesto</target>
        <note />
      </trans-unit>
      <trans-unit id="EnumProperty|OutputType|Description">
        <source>Specifies the type of application to build.</source>
        <target state="translated">Specifica il tipo di applicazione da compilare.</target>
        <note />
      </trans-unit>
      <trans-unit id="EnumProperty|OutputType|DisplayName">
        <source>Output type</source>
        <target state="translated">Tipo di output</target>
        <note />
      </trans-unit>
      <trans-unit id="EnumProperty|ResourceSpecificationKind|Description">
        <source>Specifies how application resources will be managed.</source>
        <target state="translated">Consente di specificare in che modo verranno gestite le risorse dell'applicazione.</target>
        <note />
      </trans-unit>
      <trans-unit id="EnumProperty|ResourceSpecificationKind|DisplayName">
        <source>Resources</source>
        <target state="translated">Risorse</target>
        <note />
      </trans-unit>
      <trans-unit id="EnumValue|ApplicationManifestKind.CustomManifest|DisplayName">
        <source>Use a custom manifest</source>
        <target state="translated">Usa un manifesto personalizzato</target>
        <note />
      </trans-unit>
      <trans-unit id="EnumValue|ApplicationManifestKind.DefaultManifest|DisplayName">
        <source>Embed manifest with default settings</source>
        <target state="translated">Incorpora manifesto con impostazioni predefinite</target>
        <note />
      </trans-unit>
      <trans-unit id="EnumValue|ApplicationManifestKind.NoManifest|DisplayName">
        <source>Create application without a manifest</source>
        <target state="translated">Crea applicazione senza manifesto</target>
        <note />
      </trans-unit>
      <trans-unit id="EnumValue|OutputType.Exe|DisplayName">
        <source>Console Application</source>
        <target state="translated">Applicazione console</target>
        <note />
      </trans-unit>
      <trans-unit id="EnumValue|OutputType.Library|DisplayName">
        <source>Class Library</source>
        <target state="translated">Libreria di classi</target>
        <note />
      </trans-unit>
      <trans-unit id="EnumValue|OutputType.WinExe|DisplayName">
        <source>Windows Application</source>
        <target state="translated">Applicazione Windows</target>
        <note />
      </trans-unit>
      <trans-unit id="EnumValue|ResourceSpecificationKind.IconAndManifest|DisplayName">
        <source>Icon and manifest</source>
        <target state="translated">Icona e manifesto</target>
        <note />
      </trans-unit>
      <trans-unit id="EnumValue|ResourceSpecificationKind.ResourceFile|DisplayName">
        <source>Resource file</source>
        <target state="translated">File di risorse</target>
        <note />
      </trans-unit>
      <trans-unit id="Rule|Application|Description">
        <source>Specifies the project's application settings and properties.</source>
        <target state="translated">Specifica le impostazioni e le proprietà dell'applicazione del progetto.</target>
        <note />
      </trans-unit>
      <trans-unit id="Rule|Application|DisplayName">
        <source>Application</source>
        <target state="translated">Applicazione</target>
        <note />
      </trans-unit>
      <trans-unit id="StringProperty|ApplicationIcon|Description">
        <source>Sets the .ico file that you want to use as your program icon.</source>
        <target state="translated">Imposta il file ICO da usare come icona del programma.</target>
        <note />
      </trans-unit>
      <trans-unit id="StringProperty|ApplicationIcon|DisplayName">
        <source>Icon</source>
        <target state="translated">Icona</target>
        <note />
      </trans-unit>
      <trans-unit id="StringProperty|ApplicationManifestPath|Description">
        <source>The path to the custom manifest file.</source>
        <target state="translated">Percorso del file manifesto personalizzato.</target>
        <note />
      </trans-unit>
      <trans-unit id="StringProperty|ApplicationManifestPath|DisplayName">
        <source>Manifest path</source>
        <target state="translated">Percorso del manifesto</target>
        <note />
      </trans-unit>
      <trans-unit id="StringProperty|AssemblyName|Description">
        <source>Specifies the name of the output file that will hold the assembly manifest.</source>
        <target state="translated">Specifica il nome del file di output che conterrà il manifesto dell'assembly.</target>
        <note />
      </trans-unit>
      <trans-unit id="StringProperty|AssemblyName|DisplayName">
        <source>Assembly name</source>
        <target state="translated">Nome dell'assembly</target>
        <note />
      </trans-unit>
      <trans-unit id="StringProperty|InstallOtherFrameworks|DisplayName">
        <source>Install other frameworks</source>
        <target state="translated">Installa altri framework</target>
        <note />
      </trans-unit>
      <trans-unit id="StringProperty|RootNamespace|Description">
        <source>Specifies the base namespace for files added to the project.</source>
        <target state="translated">Specifica lo spazio dei nomi di base per i file aggiunti al progetto.</target>
        <note />
      </trans-unit>
      <trans-unit id="StringProperty|RootNamespace|DisplayName">
        <source>Default namespace</source>
        <target state="translated">Spazio dei nomi predefinito</target>
        <note />
      </trans-unit>
      <trans-unit id="StringProperty|TargetFrameworks|Description">
        <source>Specifies the semicolon-delimited list of frameworks that this project will target.</source>
        <target state="translated">Consente di specificare l'elenco di framework delimitati da punto e virgola a cui verrà destinato il progetto.</target>
        <note />
      </trans-unit>
      <trans-unit id="StringProperty|TargetFrameworks|DisplayName">
        <source>Target frameworks</source>
        <target state="translated">Framework di destinazione</target>
        <note />
      </trans-unit>
      <trans-unit id="StringProperty|TargetFrameworks|Metadata|SearchTerms">
        <source>TFM</source>
        <target state="new">TFM</target>
        <note />
      </trans-unit>
      <trans-unit id="StringProperty|Win32Resource|Description">
        <source>Specifies a resource file for the project.</source>
        <target state="translated">Consente di specificare un file di risorse per il progetto.</target>
        <note />
      </trans-unit>
      <trans-unit id="StringProperty|Win32Resource|DisplayName">
        <source>Resource file</source>
        <target state="translated">File di risorse</target>
        <note />
      </trans-unit>
