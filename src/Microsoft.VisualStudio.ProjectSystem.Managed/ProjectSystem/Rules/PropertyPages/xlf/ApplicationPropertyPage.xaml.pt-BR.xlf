        <note />
      </trans-unit>
      <trans-unit id="StringProperty|RootNamespace|DisplayName">
        <source>Default namespace</source>
        <target state="translated">Namespace padrão</target>
        <note />
      </trans-unit>
      <trans-unit id="StringProperty|TargetFrameworks|Description">
        <source>Specifies the semicolon-delimited list of frameworks that this project will target.</source>
        <target state="translated">Especifica a lista delimitada por ponto e vírgula de estruturas de destino deste projeto.</target>
        <note />
      </trans-unit>
      <trans-unit id="StringProperty|TargetFrameworks|DisplayName">
        <source>Target frameworks</source>
        <target state="translated">Estruturas de destino</target>
        <note />
      </trans-unit>
      <trans-unit id="StringProperty|TargetFrameworks|Metadata|SearchTerms">
        <source>TFM</source>
        <target state="translated">TFM</target>
        <note />
      </trans-unit>
      <trans-unit id="StringProperty|Win32Resource|Description">
        <source>Specifies a resource file for the project.</source>
        <target state="translated">Especifica um arquivo de recurso para o projeto.</target>
        <note />
      </trans-unit>
      <trans-unit id="StringProperty|Win32Resource|DisplayName">
        <source>Resource file</source>
        <target state="translated">Arquivo de recurso</target>
        <note />
      </trans-unit>
