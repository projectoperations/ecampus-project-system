<?xml version="1.0" encoding="utf-8"?>
<xliff xmlns="urn:oasis:names:tc:xliff:document:1.2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.2" xsi:schemaLocation="urn:oasis:names:tc:xliff:document:1.2 xliff-core-1.2-transitional.xsd">
  <file datatype="xml" source-language="en" target-language="zh-Hant" original="../SigningPropertyPage.xaml">
    <body>
      <trans-unit id="BoolProperty|DelaySign|Description">
        <source>Use delayed signing when access to the private key is restricted. The public key will be used during the build, and addition of the private key information deferred until the assembly is handed off.</source>
        <target state="translated">在私密金鑰的存取權受限時使用延遲簽署。在建置期間將會使用公開金鑰，且私密金鑰資訊會延遲到提交組件時才新增。</target>
        <note />
      </trans-unit>
      <trans-unit id="BoolProperty|DelaySign|DisplayName">
        <source>Delay sign only</source>
        <target state="translated">僅延遲簽署</target>
        <note />
      </trans-unit>
      <trans-unit id="BoolProperty|SignAssembly|Description">
        <source>Select this check box to sign the assembly.</source>
        <target state="translated">選取此核取方塊以簽署組件。</target>
        <note />
      </trans-unit>
      <trans-unit id="BoolProperty|SignAssembly|DisplayName">
        <source>Sign the assembly</source>
        <target state="translated">簽署組件</target>
        <note />
      </trans-unit>
      <trans-unit id="Rule|SigningPropertyPage|Description">
        <source>Properties related to producing signing assemblies.</source>
        <target state="translated">與產生簽署組件相關的屬性。</target>
        <note />
      </trans-unit>
      <trans-unit id="Rule|SigningPropertyPage|DisplayName">
        <source>Signing</source>
        <target state="translated">簽署</target>
        <note />
      </trans-unit>
      <trans-unit id="StringProperty|AssemblyOriginatorKeyFile|DisplayName">
        <source>Strong name key file</source>
        <target state="translated">強式名稱金鑰檔案</target>
        <note />
      </trans-unit>
    </body>
  </file>
</xliff>